<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jikexueyuan.exam" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/yemei"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HospitalActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExamActivity"
            android:label="@string/title_activity_exam"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ResultActivity"
            android:label="@string/title_activity_result"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".Exam2Activity"
            android:label="@string/title_activity_exam2"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".Result2Activity"
            android:label="@string/title_activity_result2"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".LastActivity"
            android:label="@string/title_activity_last"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".Last2Activity"
            android:label="@string/title_activity_last2"
            android:screenOrientation="landscape" >
        </activity>
        <activity android:name=".MainActivity"
            android:screenOrientation="landscape">
        </activity>
    </application>

</manifest>
